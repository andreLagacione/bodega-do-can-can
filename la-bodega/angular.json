{
  "version": 1,
  "projects": {
    "header": {
      "projectType": "library",
      "root": "libs/header",
      "sourceRoot": "libs/header/src",
      "prefix": "la-bodega",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/header"],
          "options": {
            "jestConfig": "libs/header/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/header/src/**/*.ts",
              "libs/header/src/**/*.html"
            ]
          }
        }
      }
    },
    "main": {
      "projectType": "application",
      "root": "apps/main",
      "sourceRoot": "apps/main/src",
      "prefix": "la-bodega",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/main",
            "index": "apps/main/src/index.html",
            "main": "apps/main/src/main.ts",
            "polyfills": "apps/main/src/polyfills.ts",
            "tsConfig": "apps/main/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/main/src/favicon.ico", "apps/main/src/assets"],
            "styles": ["apps/main/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/main/src/environments/environment.ts",
                  "with": "apps/main/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "main:build:production"
            },
            "development": {
              "browserTarget": "main:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "main:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/main/src/**/*.ts",
              "apps/main/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/main"],
          "options": {
            "jestConfig": "apps/main/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "main-e2e": {
      "root": "apps/main-e2e",
      "sourceRoot": "apps/main-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/main-e2e/cypress.json",
            "devServerTarget": "main:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/main-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "main-page": {
      "projectType": "library",
      "root": "libs/main-page",
      "sourceRoot": "libs/main-page/src",
      "prefix": "la-bodega",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/main-page"],
          "options": {
            "jestConfig": "libs/main-page/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/main-page/src/**/*.ts",
              "libs/main-page/src/**/*.html"
            ]
          }
        }
      }
    },
    "root-app": {
      "projectType": "application",
      "root": "apps/root-app",
      "sourceRoot": "apps/root-app/src",
      "prefix": "la-bodega",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/root-app",
            "index": "apps/root-app/src/index.html",
            "main": "apps/root-app/src/main.ts",
            "polyfills": "apps/root-app/src/polyfills.ts",
            "tsConfig": "apps/root-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/root-app/src/favicon.ico",
              "apps/root-app/src/assets"
            ],
            "styles": ["apps/root-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/root-app/src/environments/environment.ts",
                  "with": "apps/root-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "root-app:build:production"
            },
            "development": {
              "browserTarget": "root-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "root-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/root-app/src/**/*.ts",
              "apps/root-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/root-app"],
          "options": {
            "jestConfig": "apps/root-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "root-app-e2e": {
      "root": "apps/root-app-e2e",
      "sourceRoot": "apps/root-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/root-app-e2e/cypress.json",
            "devServerTarget": "root-app:serve:development",
            "tsConfig": "apps/root-app-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "root-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/root-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "root-app"
}
