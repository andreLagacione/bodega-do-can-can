          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "bodega",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
