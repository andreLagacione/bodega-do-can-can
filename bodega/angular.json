{
  "version": 1,
  "projects": {
    "bodega": {
      "projectType": "application",
      "root": "apps/bodega",
      "sourceRoot": "apps/bodega/src",
      "prefix": "bodega",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/bodega",
            "index": "apps/bodega/src/index.html",
            "main": "apps/bodega/src/main.ts",
            "polyfills": "apps/bodega/src/polyfills.ts",
            "tsConfig": "apps/bodega/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/bodega/src/favicon.ico", "apps/bodega/src/assets"],
            "styles": ["apps/bodega/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/bodega/src/environments/environment.ts",
                  "with": "apps/bodega/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "bodega:build:production"
            },
            "development": {
              "browserTarget": "bodega:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bodega:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bodega/src/**/*.ts",
              "apps/bodega/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bodega"],
          "options": {
            "jestConfig": "apps/bodega/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "bodega-e2e": {
      "root": "apps/bodega-e2e",
      "sourceRoot": "apps/bodega-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bodega-e2e/cypress.json",
            "devServerTarget": "bodega:serve:development",
            "tsConfig": "apps/bodega-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bodega:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/bodega-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "employee": {
      "root": "libs/employee",
      "sourceRoot": "libs/employee/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
